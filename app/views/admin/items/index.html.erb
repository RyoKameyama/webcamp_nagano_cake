<h1>アイテムインデックス</h1>

<table>
<tbody>
  <td>商品ID</td>
  <td>商品名</td>
  <td>税抜価格</td>
  <td>ジャンル</td>
  <td>ステータス</td>
<% @items.each do |item| %>
<tr>
  <td><%= item.id %></td>
  <td><%= item.name %></td>
  <td><%= item.price %></td>
  <td><%= item.genre.name %></td>
  <td><%= item.is_active  %></td>
</tr>
<% end %>
</tbody>
</table>

